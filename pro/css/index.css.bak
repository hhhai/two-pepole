.my_body_img{
    width:1110px;
    height:520px;
}
.carousel-control-prev,
.carousel-control-next{
    width:4%;height:20%;
    top:40%;
    background-color:rgba(0,0,0,.3);
    border-radius:4px;

}
.carousel-indicators li{
    width:0.8rem;
    height:0.8rem;
    border-radius:50%;
    background:#fff ;
}
.carousel-indicators .active{
    background-color:#0aa1ed;
    border-radius:0.25rem;
}
.my_body_height{
   width:550px;
   height:713px;
}
